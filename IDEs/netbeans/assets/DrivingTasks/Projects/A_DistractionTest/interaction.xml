<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<interaction xmlns="http://opends.eu/drivingtask/interaction" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://opends.eu/drivingtask/interaction ../../Schema/interaction.xsd">
	<activities>
    <activity id="StartPosition_openInstructionsScreen" ref="">
                  <action delay="0.0" id="openInstructionsScreen" repeat="1">
                        <parameter name="instructionID" value="distractionScreen_1"/>
                  </action>
    </activity>
	</activities>
	<triggers>
		<trigger id="Exp_BLC_150_StartPosition_Trigger" priority="1">
                  <activities>
                        <activity ref="StartPosition_openInstructionsScreen"/>
                  </activities>
                  <condition>collideWith:Exp_BLC_150_StartPosition_Trigger</condition>
        </trigger>
	</triggers>
</interaction>
